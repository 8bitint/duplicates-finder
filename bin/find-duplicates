#!/usr/bin/env ruby
Dir[File.join('.', 'lib/*.rb')].each do |f|
  require f
end

arguments_parser = ArgumentsParser.new(ARGV)
file_iterator = FileIterator.new
duplicate_candidates_tracker = DuplicateCandidatesTracker.new
DuplicateFinder.new(arguments_parser, file_iterator, duplicate_candidates_tracker).run
